main()
 begin 
    int x;

    x <- 0;
    if true then 
        output(666);
    else while x<10 do  begin 
        output(x);
        x <- x+1;
     end 

    x <- 0;
    if false then 
        output(666);
    else while x<10 do  begin 
        output(x);
        x <- x+1;
     end 

 end 
