main()
 begin 
      int n;

      while true do  begin 
          n <- input();
          if n<2 then break;

          while n%2=0 do  begin 
              output(2);
              n /= 2;
           end 

          for i <- 3 to n by 2 do  begin 
              if i*i>n then break;
              while n%i=0 do  begin 
                  output(i);
                  n /= i;
               end 
           end 
          if n>1 then output(n);
          outnl();
       end 
 end 
